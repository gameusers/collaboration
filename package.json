  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "mongo-express": "^0.46.1",
    "mongoose": "^5.0.14",
    "next": "^5.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
